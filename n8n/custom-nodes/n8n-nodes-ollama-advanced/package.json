{
  "name": "n8n-nodes-ollama-advanced",
  "version": "1.0.0",
  "description": "Advanced Ollama integration for n8n with AI Agent support and recursive summarization",
  "keywords": [
    "n8n-community-node-package",
    "ollama",
    "llm",
    "ai",
    "qwen",
    "llama",
    "mistral",
    "deepseek"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-ollama-advanced",
  "author": {
    "name": "Your Name",
    "email": "your@email.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-ollama-advanced.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes package.json",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.test.ts'",
    "test:integration": "node test/integration/test-ollama.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/OllamaAdvanced/OllamaAdvanced.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}